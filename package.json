    "@babel/polyfill": "^7.12.1",
    "@ckeditor/ckeditor5-build-classic": "^16.0.0",
    "@ckeditor/ckeditor5-ckfinder": "^16.0.0",
    "choices.js": "^9.0.1",
    "core-js": "^3.13.1",
    "flatpickr": "^4.6.9",
    "nouislider": "^14.7.0",
    "tui-image-editor": "^3.14.3",
    "uikit": "^3.6.22"
